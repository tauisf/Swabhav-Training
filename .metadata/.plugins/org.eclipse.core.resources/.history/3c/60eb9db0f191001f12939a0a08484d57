package com.aurionpro.decorator.model;

public class CarService implements ICarService{
	 
   static int cost;
	public  void  startService() {

		ICarService carService = new CarInspectionService();
		carService.getCost();
		
	}
	
	public double getOilChange(ICarService service) {
		
		ICarService oilService = new OilChange();
		return  oilService.getCost();
	}
	public double getWheelALign(ICarService carService) {
			
			ICarService wheelService = new WheelAlignment();
			return    wheelService.getCost();
		}

	@Override
	public double getCost() {
		
		return 0;
	}
	
	
}
