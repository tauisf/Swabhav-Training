package com.aurionpro.facade.model;

public class AccountService {

	
	public void createSavingAccount(int id ,String name ,double balance) {
        IAccountFactory factory = new CreateSavingAccount();
		 
		 IAccount  account = factory.createAccount(id,name,balance);
		 
		 System.out.println("Account created  successfull");
	}
}
