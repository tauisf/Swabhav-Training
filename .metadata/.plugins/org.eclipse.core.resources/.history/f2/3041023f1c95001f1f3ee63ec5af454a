package com.aurionpro.tdd;

public class AreaCalculator {

	double CalculateAreaOfCircle(double radius) {
		return 3.14*(radius*radius);
	}
	double CalculateAreaOfSquare(double radius) {
		return 3.14*(radius*radius);
	}
	double CalculateAreaOFRectangle(double radius) {
		return 3.14*(radius*radius);
	}
}
