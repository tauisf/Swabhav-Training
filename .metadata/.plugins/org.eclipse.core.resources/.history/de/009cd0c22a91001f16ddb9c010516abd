package com.aurionpro.account.model;

public class CurrentAccount implements IAccount  {
	private int id ;
	private String name;
	
	
	public CurrentAccount(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}


	@Override
	public void credit(double amount) {
		System.out.println("the account credit in current account "+ amount);
		
	}

	@Override
	public void debit(double amount) {
		System.out.println("the account debit  in current account "+ amount);
		
	}


	@Override
	public String toString() {
		return "CurrentAccount [id=" + id + ", name=" + name + "]";
	}
	
	

}
