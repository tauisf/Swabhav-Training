package com.aurionpro.account.model;

public class SavingAccount implements IAccount{
	private int id ;
	private String name;

	

	public SavingAccount(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	@Override
	public void credit(double amount) {
		System.out.println("the account credit in saving "+ amount);
		
	}

	@Override
	public void debit(double amount) {
		System.out.println("the account debit from  saving "+amount);
		
	}

}
